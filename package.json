{
  "name": "ai-data-extractor",
  "private": true,
  "version": "1.9.0",
  "productName": "AI Data Extractor",
  "type": "module",
  "main": "dist-electron/main.js",
  "author": "Pablo D Ojeda M",
  "license": "UNLICENSED",
  "description": "AI Data Extractor - An√°lisis de datos con IA",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:electron",
    "build:electron": "tsc -p electron/tsconfig.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "npm run build:electron && wait-on tcp:5173 && cross-env NODE_ENV=development electron .",
    "electron:pack": "npm run build && electron-builder",
    "start": "concurrently -k \"npm run dev\" \"npm run electron\""
  },
  "build": {
    "appId": "com.extractor.ai",
    "productName": "AI Data Extractor",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowElevation": true
    }
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}